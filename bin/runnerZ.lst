                                	MOVR R2,R4
                                	ANDI #$3F,R4
                                	MVO R4,_music_n2	; Note
                                	MVO R3,_music_s2	; Waveform
                                	ANDI #$C0,R2
                                	MVO R2,_music_i2	; Instrument
                                	
                                @@2:	MOVR R1,R2
